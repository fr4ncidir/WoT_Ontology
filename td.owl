    <!-- http://wot.arces.unibo.it/ontology/web_of_things#InputData -->

    <owl:Class rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#InputData">
        <rdfs:subClassOf rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#Data"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An Action may require some input to be performed. The input is related to the particular action, and characterized the actual instance</rdfs:comment>
    </owl:Class>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#OutputData -->

    <owl:Class rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#OutputData">
        <rdfs:subClassOf rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#Data"/>
        <web_of_things:add_output_data rdf:datatype="http://www.semanticweb.org/francesco/ontologies/2017/9/sepa_annotations#SPARQL">&quot;ADD_OUTPUT_DATA&quot;:{
&quot;sparql&quot;:&quot;DELETE {?item td:hasOutputData ?oldOoutput.
?output td:hasTimestamp ?oldTimeStamp.
?output dul:hasDataValue ?oldValue}
INSERT {?item td:hasOutputData ?newOutput.
?newOutput td:hasTimestamp ?newTimeStamp.
?newOutput dul:hasDataValue ?newValue}
WHERE {
{{?item rdf:type td:ActionInstance} UNION {?item rdf:type td:Event}}.
OPTIONAL{?item td:hasOutputData ?oldOoutput.
?output td:hasTimestamp ?oldTimeStamp.
?output dul:hasDataValue ?oldValue
}.
BIND(now() AS ?newTimeStamp)
}&quot;,
&quot;forcedBindings&quot;:{
&quot;item&quot;:{&quot;type&quot;:&quot;uri&quot;,&quot;value&quot;:&quot;&quot;},
&quot;newOutput&quot;:{&quot;type&quot;:&quot;uri&quot;,&quot;value&quot;:&quot;&quot;},
&quot;newValue&quot;:{&quot;type&quot;:&quot;literal&quot;,&quot;value&quot;:&quot;&quot;},
}
}</web_of_things:add_output_data>
        <rdfs:comment rdf:datatype="http://www.semanticweb.org/francesco/ontologies/2017/9/sepa_annotations#SPARQL">Actions and Events may result in some data. Such data is collected within OutputData instance</rdfs:comment>
    </owl:Class>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#Property -->

    <owl:Class rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#Property">
        <web_of_things:add_properties rdf:datatype="http://www.semanticweb.org/francesco/ontologies/2017/9/sepa_annotations#SPARQL">&quot;ADD_PROPERTY&quot;:{
&quot;sparql&quot;:&quot;DELETE {?thing td:hasProperty ?property.
