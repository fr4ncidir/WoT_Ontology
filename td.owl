    <!-- http://wot.arces.unibo.it/ontology/web_of_things#forProperty -->

    <owl:ObjectProperty rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#forProperty">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#Action"/>
                    <rdf:Description rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#Event"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#Property"/>
        <web_of_things:add_forproperty rdf:datatype="http://www.semanticweb.org/francesco/ontologies/2017/9/sepa_annotations#SPARQL">&quot;INSERT_FORPROPERTY&quot;:{
&quot;sparql&quot;:&quot;INSERT {?item td:forProperty ?property}
WHERE {?property rdf:type td:Property.
{?item rdf:type td:Action} UNION {?item rdf:type td:Event}}&quot;,
&quot;forcedBindings&quot;:{
&quot;item&quot;:{&quot;type&quot;:&quot;uri&quot;,&quot;value&quot;:&quot;&quot;},
&quot;property&quot;:{&quot;type&quot;:&quot;uri&quot;,&quot;value&quot;:&quot;&quot;}
}
}</web_of_things:add_forproperty>
        <web_of_things:delete_forproperty rdf:datatype="http://www.semanticweb.org/francesco/ontologies/2017/9/sepa_annotations#SPARQL">&quot;DELETE_FORPROPERTY&quot;:{
&quot;sparql&quot;:&quot;DELETE {?item td:forProperty ?property}
WHERE {?property rdf:type td:Property.
{?item rdf:type td:Action} UNION {?item rdf:type td:Event}}&quot;,
&quot;forcedBindings&quot;:{
&quot;item&quot;:{&quot;type&quot;:&quot;uri&quot;,&quot;value&quot;:&quot;&quot;},
}
}</web_of_things:delete_forproperty>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An Action or an Event may refer to a Property.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#hasAction -->

    <owl:ObjectProperty rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#hasAction">
        <rdfs:domain rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#Thing"/>
        <rdfs:range rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#Action"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A Thing may be able to perform a task in a finite amount of time: the task is the Action</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#hasActionInstance -->

    <owl:ObjectProperty rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#hasActionInstance">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#Action"/>
        <rdfs:range rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#ActionInstance"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">When you create an ActionInstance, you ask a Thing to perform and Action</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#hasEvent -->

    <owl:ObjectProperty rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#hasEvent">
        <rdfs:domain rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#Thing"/>
        <rdfs:range rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#Event"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A Thing may be able to notify an Event</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#hasInputData -->

    <owl:ObjectProperty rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#hasInputData">
        <rdfs:domain rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#ActionInstance"/>
        <rdfs:range rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#InputData"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Action may need some input values to perform.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#hasOutputData -->

    <owl:ObjectProperty rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#hasOutputData">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#ActionInstance"/>
                    <rdf:Description rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#Event"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#OutputData"/>
        <web_of_things:add_new_outputdata rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&quot;ADD_OUTPUT_DATA&quot;:{
&quot;sparql&quot;:&quot;INSERT {?item td:hasOutputData ?outputData. ?outputData td:hasTimeStamp ?time. ?outputData dul:hasDataValue ?value } WHERE { BIND(now() AS ?time). {?item rdf:type td:ActionInstance} UNION {?item rdf:type td:Event}}&quot;,
&quot;forcedBindings&quot;:{
&quot;item&quot;: {
&quot;type&quot;:&quot;uri&quot;,
&quot;value&quot;:&quot;&quot;
},
&quot;outputData&quot;:{
&quot;type&quot;:&quot;uri&quot;,
&quot;value&quot;:&quot;&quot;
},
}
}</web_of_things:add_new_outputdata>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Events and Actions may give some output as a result of their execution.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#hasProperty -->

    <owl:ObjectProperty rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#hasProperty">
