        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#Name"/>
        <web_of_things:get_thing_by_name rdf:datatype="http://www.semanticweb.org/francesco/ontologies/2017/9/sepa_annotations#SPARQL">&quot;GET_THING_BY_NAME&quot;:{
&quot;sparql&quot;:&quot;SELECT ?thing WHERE {?thing rdf:type td:Thing. ?thing td:hasName ?name}&quot;,
&quot;forcedBindings&quot;:{
&quot;name&quot;:{
&quot;type&quot;:&quot;literal&quot;,
&quot;value&quot;:&quot;&quot;
}
