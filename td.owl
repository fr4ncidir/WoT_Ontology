        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#</rdfs:isDefinedBy>
    </owl:DatatypeProperty>
    


