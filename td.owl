        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#long"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A Property value may be stable for a certain period of time, expressed in ms. We use 0 to say that it changes asynchronously.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#hasTimeStamp -->

    <owl:DatatypeProperty rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#hasTimeStamp">
