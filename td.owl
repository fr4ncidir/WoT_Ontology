        <web_of_things:delete_event rdf:datatype="http://www.semanticweb.org/francesco/ontologies/2017/9/sepa_annotations#SPARQL">&quot;DELETE_EVENT&quot;:{
&quot;sparql&quot;:&quot;DELETE {?thing td:hasEvent ?event.
?event td:hasName ?name.
?event rdf:type td:Event}
WHERE {?thing rdf:type td:Thing}&quot;,
&quot;forcedBindings&quot;:{
&quot;thing&quot;:{&quot;type&quot;:&quot;uri&quot;,&quot;value&quot;:&quot;&quot;},
&quot;thing&quot;:{&quot;event&quot;:&quot;uri&quot;,&quot;value&quot;:&quot;&quot;},
}
}</web_of_things:delete_event>
        <web_of_things:get_event_notification rdf:datatype="http://www.semanticweb.org/francesco/ontologies/2017/9/sepa_annotations#SPARQL">&quot;GET_EVENT_NOTIFICATION&quot;:{
&quot;sparql&quot;:&quot;SELECT ?event_time ?output ?output_time ?value
WHERE {
?event rdf:type td:Event.
?event td:hasTimeStamp ?event_time.
OPTIONAL{?event td:hasOutputData ?output.
?output td:hasTimeStamp ?output_time.
?output dul:hasDataValue ?value}}&quot;,
&quot;forcedBindings&quot;:{
&quot;event&quot;:{&quot;type&quot;:&quot;uri&quot;,&quot;value&quot;:&quot;&quot;}
}
