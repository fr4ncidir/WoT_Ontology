        <web_of_things:add_property_changed_event rdf:datatype="http://www.semanticweb.org/francesco/ontologies/2017/9/sepa_annotations#SPARQL">&quot;ADD_EVENT&quot;:{
&quot;sparql&quot;:&quot;DELETE {?event td:hasName ?oldName}
INSERT {?thing td:hasEvent ?event.
?event rdf:type td:Event.
?event rdf:type td:PropertyChangedEvent.
?event td:hasName ?newName}
WHERE {?thing rdf:type td:Thing. OPTIONAL{?event td:hasName ?oldName}}&quot;,
&quot;forcedBindings&quot;:{
&quot;event&quot;:{&quot;type&quot;:&quot;uri&quot;,&quot;value&quot;:&quot;&quot;},
&quot;thing&quot;:{&quot;type&quot;:&quot;uri&quot;,&quot;value&quot;:&quot;&quot;},
&quot;newName&quot;:{&quot;type&quot;:&quot;literal&quot;,&quot;value&quot;:&quot;&quot;},
