    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- dul:hasComponent -->

    <owl:ObjectProperty rdf:about="dul:hasComponent">
        <rdfs:domain rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#Thing"/>
        <rdfs:range rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#Thing"/>
    </owl:ObjectProperty>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#forProperty -->

    <owl:ObjectProperty rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#forProperty">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#Action"/>
                    <rdf:Description rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#Event"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#Property"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An Action or an Event may refer to a Property.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#hasAction -->

    <owl:ObjectProperty rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#hasAction">
        <rdfs:domain rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#Thing"/>
        <rdfs:range rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#Action"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A Thing may be able to perform a task in a finite amount of time: the task is the Action</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#hasActionInstance -->

    <owl:ObjectProperty rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#hasActionInstance">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#Action"/>
        <rdfs:range rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#ActionInstance"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">When you create an ActionInstance, you ask a Thing to perform and Action</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#hasEvent -->

    <owl:ObjectProperty rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#hasEvent">
        <rdfs:domain rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#Thing"/>
        <rdfs:range rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#Event"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A Thing may be able to notify an Event</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#hasInputData -->

    <owl:ObjectProperty rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#hasInputData">
        <rdfs:domain rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#ActionInstance"/>
        <rdfs:range rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#InputData"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Action may need some input values to perform.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#hasOutputData -->

    <owl:ObjectProperty rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#hasOutputData">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#ActionInstance"/>
                    <rdf:Description rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#Event"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#OutputData"/>
        <web_of_things:add_new_outputdata rdf:datatype="http://www.w3.org/2001/XMLSchema#string">&quot;ADD_OUTPUT_DATA&quot;:{
&quot;sparql&quot;:&quot;INSERT {?item td:hasOutputData ?outputData. ?outputData td:hasTimeStamp ?time. ?outputData dul:hasDataValue ?value } WHERE { BIND(now() AS ?time). {?item rdf:type td:ActionInstance} UNION {?item rdf:type td:Event}}&quot;,
&quot;forcedBindings&quot;:{
&quot;item&quot;: {
&quot;type&quot;:&quot;uri&quot;,
&quot;value&quot;:&quot;&quot;
},
&quot;outputData&quot;:{
&quot;type&quot;:&quot;uri&quot;,
&quot;value&quot;:&quot;&quot;
},
}
}</web_of_things:add_new_outputdata>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Events and Actions may give some output as a result of their execution.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#hasProperty -->

    <owl:ObjectProperty rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#hasProperty">
        <rdfs:domain rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#Thing"/>
        <rdfs:range rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#Property"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A Thing may have some Properties</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- dul:hasDataValue -->

    <owl:DatatypeProperty rdf:about="dul:hasDataValue">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#InputData"/>
                    <rdf:Description rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#OutputData"/>
                    <rdf:Description rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#Property"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#anyURI"/>
                    <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#string"/>
                </owl:unionOf>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#hasConfirmationTimeStamp -->

    <owl:DatatypeProperty rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#hasConfirmationTimeStamp">
        <rdfs:subPropertyOf rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#hasTimeStamp"/>
        <rdfs:domain rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#ActionInstance"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTimeStamp"/>
        <web_of_things:add_confirmation_timestamp rdf:datatype="http://www.semanticweb.org/francesco/ontologies/2017/9/sepa_annotations#SPARQL">&quot;ADD_CONFIRMATION_TIMESTAMP&quot;:{
&quot;sparql&quot;:&quot;INSERT {?instance rdf:type td:ActionInstance.
?instance td:hasConfirmationTimeStamp ?time}
WHERE {BIND(now() AS ?time)}&quot;,
&quot;forcedBindings&quot;:{
&quot;instance&quot;:{
&quot;type&quot;:&quot;uri&quot;,
 &quot;value&quot;:&quot;&quot;
}
}
}</web_of_things:add_confirmation_timestamp>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This timestamp is set when the Thing requested for the action has accepted the instance for execution</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#hasDataSchema -->

    <owl:DatatypeProperty rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#hasDataSchema">
        <rdfs:domain rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#Property"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <web_of_things:dataschema_discovery rdf:datatype="http://www.semanticweb.org/francesco/ontologies/2017/9/sepa_annotations#SPARQL">&quot;ALL_DATASCHEMA_DISCOVERY&quot;:{
&quot;sparql&quot;:&quot;SELECT ?item ?itemType ?dataSchema ?inputDS ?outputDS WHERE {
OPTIONAL{?item td:hasDataSchema ?dataSchema}.
OPTIONAL{?item td:hasInputDataSchema ?inputDS}.
OPTIONAL{?item td:hasOutputDataSchema ?outputDS}.
?item rdf:type ?itemType
}&quot;
}</web_of_things:dataschema_discovery>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Format schema for the value of a Property</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#hasInputDataSchema -->

    <owl:DatatypeProperty rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#hasInputDataSchema">
        <rdfs:subPropertyOf rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#hasDataSchema"/>
        <rdfs:domain rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#Action"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Format schema for input data to be given to actions</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#hasName -->

    <owl:DatatypeProperty rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#hasName">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#Action"/>
                    <rdf:Description rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#Event"/>
                    <rdf:Description rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#Property"/>
                    <rdf:Description rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#Thing"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#Name"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Human readable name of Things, Actions, Properties and Events</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#hasOutputDataSchema -->

    <owl:DatatypeProperty rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#hasOutputDataSchema">
        <rdfs:subPropertyOf rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#hasDataSchema"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#Action"/>
                    <rdf:Description rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#Event"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Format schema that output of Events and Actions have.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#hasRequestTimeStamp -->

    <owl:DatatypeProperty rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#hasRequestTimeStamp">
        <rdfs:subPropertyOf rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#hasTimeStamp"/>
        <rdfs:domain rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#ActionInstance"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTimeStamp"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This timestamp is set by the requester of an Action.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#hasStability -->

    <owl:DatatypeProperty rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#hasStability">
        <rdfs:domain rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#Property"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#long"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A Property value may be stable for a certain period of time, expressed in ms. We use 0 to say that it changes asynchronously</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#hasTimeStamp -->

    <owl:DatatypeProperty rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#hasTimeStamp">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#Event"/>
                    <rdf:Description rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#OutputData"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTimeStamp"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Identifies when an OutputData is ready, or when an Event is thrown</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#isWritable -->

    <owl:DatatypeProperty rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#isWritable">
        <rdfs:domain rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#Property"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
