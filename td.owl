        <add_dataschema rdf:datatype="http://wot.arces.unibo.it/ontology/web_of_things#SPARQL_jsap">&quot;ADD_DATASCHEMA&quot;:{&quot;sparql&quot;:&quot;DELETE {?property td:hasDataSchema ?oldDataSchema} INSERT {?property td:hasDataSchema ?newDataSchema} WHERE {?property rdf:type td:Property}&quot;,&quot;forcedBindings&quot;:{&quot;property&quot;:{&quot;type&quot;:&quot;uri&quot;,&quot;value&quot;:&quot;&quot;},&quot;newDataSchema&quot;:{&quot;type&quot;:&quot;literal&quot;,&quot;value&quot;:&quot;&quot;}}}</add_dataschema>
        <dataschema_discovery rdf:datatype="http://wot.arces.unibo.it/ontology/web_of_things#SPARQL_jsap">&quot;ALL_DATASCHEMA_DISCOVERY&quot;:{&quot;sparql&quot;:&quot;SELECT ?item ?itemType ?dataSchema ?inputDS ?outputDS WHERE {OPTIONAL{?item td:hasDataSchema ?dataSchema}. OPTIONAL{?item td:hasInputDataSchema ?inputDS}. OPTIONAL{?item td:hasOutputDataSchema ?outputDS}. ?item rdf:type ?itemType}&quot;}</dataschema_discovery>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Format schema for the value of a Property</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#hasInputDataSchema -->

    <owl:DatatypeProperty rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#hasInputDataSchema">
