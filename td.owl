    <!-- dul:hasDataValue -->

    <owl:DatatypeProperty rdf:about="dul:hasDataValue">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#InputData"/>
                    <rdf:Description rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#OutputData"/>
                    <rdf:Description rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#Property"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#anyURI"/>
                    <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#string"/>
                </owl:unionOf>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#hasConfirmationTimeStamp -->

    <owl:DatatypeProperty rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#hasConfirmationTimeStamp">
        <rdfs:subPropertyOf rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#hasTimeStamp"/>
        <rdfs:domain rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#ActionInstance"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTimeStamp"/>
        <web_of_things:add_confirmation_timestamp rdf:datatype="http://www.semanticweb.org/francesco/ontologies/2017/9/sepa_annotations#SPARQL">&quot;ADD_CONFIRMATION_TIMESTAMP&quot;:{
&quot;sparql&quot;:&quot;INSERT {?instance rdf:type td:ActionInstance.
?instance td:hasConfirmationTimeStamp ?time}
WHERE {BIND(now() AS ?time)}&quot;,
&quot;forcedBindings&quot;:{
&quot;instance&quot;:{
