<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#"
     xml:base="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things"
     xmlns:td="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#"
     xmlns:adt="http://www.semanticweb.org/francesco/ontologies/2017/9/sepa_annotations#"
     xmlns:prot="http://www.semanticweb.org/francesco/ontologies/2017/9/protocol_ontology#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:dul="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#"
     xmlns:ire="http://w3c.github.io/wot/w3c-wot-td-ire.owl#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things">
        <owl:imports rdf:resource="http://www.semanticweb.org/francesco/ontologies/2017/9/protocol_ontology"/>
        <owl:imports rdf:resource="http://www.semanticweb.org/francesco/ontologies/2017/9/sepa_annotations"/>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2017-10-22T16:50:00</owl:versionInfo>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Web Of Things Ontology by ARCES WOT group</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- dul:hasComponent -->

    <owl:ObjectProperty rdf:about="dul:hasComponent">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#Thing"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#Thing"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#forProperty -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#forProperty">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#Action"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#Event"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#Property"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An Action or an Event may refer to a Property.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#hasAction -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#hasAction">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#Thing"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#Action"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A Thing may be able to perform a task in a finite amount of time: the task is the Action</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#hasActionInstance -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#hasActionInstance">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#hasInstance"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#Action"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#ActionInstance"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#hasDataSchema -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#hasDataSchema">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#Property"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#DataSchema"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#hasEvent -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#hasEvent">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#Thing"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#Event"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A Thing may be able to notify an Event</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#hasEventInstance -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#hasEventInstance">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#hasInstance"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#Event"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#EventInstance"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#hasInputData -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#hasInputData">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#ActionInstance"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#InputData"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#hasInputDataSchema -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#hasInputDataSchema">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#hasDataSchema"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#Action"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#DataSchema"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#hasInstance -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#hasInstance"/>
    


    <!-- http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#hasOutputData -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#hasOutputData">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#ActionInstance"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#EventInstance"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#OutputData"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#hasOutputDataSchema -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#hasOutputDataSchema">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#hasDataSchema"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#Action"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#Event"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#DataSchema"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#hasProperty -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#hasProperty">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#Thing"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#Property"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A Thing may have some Properties</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#isAccessibleBy -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#isAccessibleBy">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#Action"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#Thing"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.semanticweb.org/francesco/ontologies/2017/9/protocol_ontology#Protocol"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An Action, or a whole Thing, can be reached by using some specific Protocols</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- ire:hasWebLocation -->

    <owl:ObjectProperty rdf:about="ire:hasWebLocation">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#DataSchema"/>
        <rdfs:range rdf:resource="ire:AbstractWebLocation"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- dul:hasDataValue -->

    <owl:DatatypeProperty rdf:about="dul:hasDataValue">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#InputData"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#OutputData"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#Property"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#anyURI"/>
                    <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#string"/>
                </owl:unionOf>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#hasName -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#hasName">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#Action"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#Event"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#Property"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#Thing"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#Name"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#hasStability -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#hasStability">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#Property"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#long"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A Property value may be stable for a certain period of time, expressed in ms. We use 0 to say that it changes asynchronously</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#hasTimeStamp -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#hasTimeStamp">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#Instance"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTimeStamp"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Identifies when an Instance is thrown</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#isDiscoverable -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#isDiscoverable">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#Thing"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#isWritable -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#isWritable">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#Property"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">If a Property value can be changed through the use of an Action, the value should be &apos;true&apos;. Else, it should be &apos;false&apos;.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#Action -->

    <owl:Class rdf:about="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#Action">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A Thing may be able to perform Actions. Some actions may include as effect the modification of a Property owned by the same Thing.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#ActionInstance -->

    <owl:Class rdf:about="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#ActionInstance">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#Instance"/>
        <adt:query rdf:datatype="http://www.semanticweb.org/francesco/ontologies/2017/9/sepa_annotations#SPARQL">SELECT ?aInputDS ?aOutputData ?aOutputValue WHERE {
?action td:hasInstance &lt;actionInstance&gt;.
?action td:hasInputDataSchema ?aInputDS.
&lt;actionInstance&gt; td:hasOutputData ?aOutputData.
?aOutputData dul:hasDataValue ?aOutputValue
}</adt:query>
        <adt:query rdf:datatype="http://www.semanticweb.org/francesco/ontologies/2017/9/sepa_annotations#SPARQL">SELECT ?aOutputDataSchema ?aInstance ?timeStamp ?inputData ?inputValue WHERE {
&lt;action&gt; td:hasActionInstance ?aInstance.
?aInstance rdf:type td:ActionInstance.
?aInstance rdf:type td:Instance.
?aInstance td:hasTimeStamp ?timeStamp.

OPTIONAL {
&lt;action&gt; td:hasInputDataSchema ?aInputDataSchema.
?aInstance td:hasInputData ?inputData.
?inputData dul:hasDataValue ?inputValue.
}.
}</adt:query>
        <adt:update rdf:datatype="http://www.semanticweb.org/francesco/ontologies/2017/9/sepa_annotations#SPARQL">DELETE {
&lt;action&gt; td:hasInstance ?oldInstance.
?oldInstance rdf:type td:ActionInstance.
?oldInstance rdf:type td:Instance.
?oldInstance td:hasTimeStamp ?oldTS.
{?oldInstance td:hasInputData ?oldInput.
?oldInput rdf:type td:InputData.
?oldInput dul:hasDataValue ?oldInValue.}+
{?oldInstance td:hasOutputData ?oldOutput.
?oldOutput rdf:type td:OutputData.
?oldOutput dul:hasDataValue ?oldOutValue.}+
}

INSERT {
&lt;event&gt; td:hasInstance ?newInstance.
?newInstance rdf:type td:ActionInstance.
?newInstance rdf:type td:Instance.
?newInstance td:hasTimeStamp ?newTS.
{?newInstance td:hasInputData ?newInput.
?newInput rdf:type td:InputData.
?newInput dul:hasDataValue ?newValue.}+
{?newInstance td:hasOutputData ?newOutput.
?newOutput rdf:type td:OutputData.
?newOutput dul:hasDataValue ?newOutValue.}+
}

WHERE {
&lt;event&gt; rdf:type td:Action.
OPTIONAL {
&lt;event&gt; td:hasInstance ?oldInstance.
?oldInstance rdf:type td:ActionInstance.
?oldInstance td:hasTimeStamp ?oldTS.
{?oldInstance td:hasInputData ?oldInput.
?oldInput rdf:type td:InputData.
?oldInput dul:hasDataValue ?oldValue.}+
{?oldInstance td:hasOutputData ?oldOutput.
?oldOutput rdf:type td:OutputData.
?oldOutput dul:hasDataValue ?oldOutValue.}+
}.
}</adt:update>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">To request the execution of an Action, an ActionInstance must be defined</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#Data -->

    <owl:Class rdf:about="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#Data"/>
    


    <!-- http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#DataSchema -->

    <owl:Class rdf:about="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#DataSchema">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A DataSchema identifies a way of encoding a piece of information. So, an InputData, an OutputData as well as a Property values must refer to a DataSchema to be correctly interpreted</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#Event -->

    <owl:Class rdf:about="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#Event">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A Thing may be able to signal the occurrence of particular Events.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#EventInstance -->

    <owl:Class rdf:about="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#EventInstance">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#Instance"/>
        <adt:query rdf:datatype="http://www.semanticweb.org/francesco/ontologies/2017/9/sepa_annotations#SPARQL">SELECT ?eOutputDataSchema ?eInstance ?timeStamp ?outputData ?outputValue WHERE {
&lt;event&gt; td:hasEventInstance ?eInstance.
?eInstance rdf:type td:EventInstance.
?eInstance rdf:type td:Instance.
?eInstance td:hasTimeStamp ?timeStamp.

OPTIONAL {
&lt;event&gt; td:hasOutputSchema ?eOutputDataSchema.
?eInstance td:hasOutputData ?outputData.
?outputData dul:hasDataValue ?outputValue.
}.
}</adt:query>
        <adt:update rdf:datatype="http://www.semanticweb.org/francesco/ontologies/2017/9/sepa_annotations#SPARQL">DELETE {
&lt;event&gt; td:hasInstance ?oldInstance.
?oldInstance rdf:type td:EventInstance.
?oldInstance rdf:type td:Instance.
?oldInstance td:hasTimeStamp ?oldTS.
?oldInstance td:hasOutputData ?oldOutput.
?oldOutput rdf:type td:OutputData.
?oldOutput dul:hasDataValue ?oldValue.
}

INSERT {
&lt;event&gt; td:hasInstance ?newInstance.
?newInstance rdf:type td:EventInstance.
?newInstance rdf:type td:Instance.
?newInstance td:hasTimeStamp ?newTS.
?newInstance td:hasOutputData ?newOutput.
?newOutput rdf:type td:OutputData.
?newOutput dul:hasDataValue ?newValue.
}

WHERE {
&lt;event&gt; rdf:type td:Event.
OPTIONAL {
&lt;event&gt; td:hasInstance ?oldInstance.
?oldInstance rdf:type td:EventInstance.
?oldInstance td:hasTimeStamp ?oldTS.
?oldInstance td:hasOutputData ?oldOutput.
?oldOutput rdf:type td:OutputData.
?oldOutput dul:hasDataValue ?oldValue.
}.
}</adt:update>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">To throw an Event, you must creat an EventInstance</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#InputData -->

    <owl:Class rdf:about="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#InputData">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#Data"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#Instance -->

    <owl:Class rdf:about="http://www.semanticweb.org/francesco/ontologies/2017/9/web_of_things#Instance">
        <adt:discovery rdf:datatype="http://www.semanticweb.org/francesco/ontologies/2017/9/sepa_annotations#SPARQL">SELECT ?instance ?iOrigin ?iType ?thing WHERE {
?instance rdf:type td:Instance.
?iOrigin td:hasInstance ?instance.
?thing rdf:type td:Thing.
?thing ?iType ?iOrigin.
}</adt:discovery>
