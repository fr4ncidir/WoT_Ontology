&quot;sparql&quot;:&quot;DELETE {?item td:hasOutputDataSchema ?oldDataSchema} INSERT {?item td:hasOutputDataSchema ?newDataSchema} WHERE {{?item rdf:type td:Action} UNION {?item rdf:type }}&quot;,
&quot;forcedBindings&quot;:{
