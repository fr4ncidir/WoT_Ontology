    <!-- dul:hasDataValue -->

    <owl:DatatypeProperty rdf:about="dul:hasDataValue">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#InputData"/>
                    <rdf:Description rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#OutputData"/>
