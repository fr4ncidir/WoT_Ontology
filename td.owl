        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#Name"/>
        <get_thing_by_name rdf:datatype="http://wot.arces.unibo.it/ontology/web_of_things#SPARQL_jsap">&quot;GET_THING_BY_NAME&quot;:{&quot;sparql&quot;:&quot;SELECT ?thing WHERE {?thing rdf:type td:Thing. ?thing td:hasName ?name}&quot;,&quot;forcedBindings&quot;:{&quot;name&quot;:{&quot;type&quot;:&quot;literal&quot;,&quot;value&quot;:&quot;&quot;}}}</get_thing_by_name>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Human readable name of Things, Actions, Properties and Events</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#hasOutputDataSchema -->

    <owl:DatatypeProperty rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#hasOutputDataSchema">
        <rdfs:subPropertyOf rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#hasDataSchema"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#Action"/>
                    <rdf:Description rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#Event"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <add_dataschema rdf:datatype="http://wot.arces.unibo.it/ontology/web_of_things#SPARQL_jsap">&quot;ADD_OUTPUT_DATASCHEMA&quot;:{&quot;sparql&quot;:&quot;DELETE {?item td:hasOutputDataSchema ?oldDataSchema} INSERT {?item td:hasOutputDataSchema ?newDataSchema} WHERE {{?item rdf:type td:Action} UNION {?item rdf:type }}&quot;,&quot;forcedBindings&quot;:{&quot;property&quot;:{ &quot;type&quot;:&quot;uri&quot;,&quot;value&quot;:&quot;&quot;},&quot;newDataSchema&quot;:{&quot;type&quot;:&quot;literal&quot;,&quot;value&quot;:&quot;&quot;}}}</add_dataschema>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Format schema that have outputs of Events and Actions</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#hasRequestTimeStamp -->

    <owl:DatatypeProperty rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#hasRequestTimeStamp">
