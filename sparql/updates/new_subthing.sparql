#   
#   Appends to a preexistent thing a subthing relationship to another
#   preexistent thing

NEW_SUBTHING:
    sparql: "
        insert { ?superthing wot:hasSubThing ?subthing }
        where { 
            ?superthing rdf:type wot:Thing.
            ?subthing rdf:type wot:Thing }"
    forcedBindings:
    superthing:
        type: uri
        value: ""
    subthing:
        type: uri
        value: ""
