#   
#   DataSchema discovery

GET_DATASCHEMAS:
    sparql: "
        select * 
        where {
            ?ds rdf:type wot:DataSchema.
            ?ip rdf:type wot:InteractionPattern;
                ?dataschema_type ?ds.
            ?ds wot:hasFieldSchema ?fs.
            ?fs rdf:type ?fs_type.
            values (?ip ?ds ?fs) {(?ip_force ?ds_force ?fs_force)} 
            filter (?fs_type != wot:FieldSchema)
            values (?fs_type) {(?fs_type_force)} 
        }"
    forcedBindings:
        dataschema_type:
            type: uri
            value: wot:hasInputDataSchema|wot:hasOutputDataSchema|wot:hasPropertyDataSchema
        ip_force:
            type: uri
            value: UNDEF
        ds_force:
            type: uri
            value: UNDEF
        fs_force:
            type: uri
            value: UNDEF
        fs_type_force:
            type: uri
            value: UNDEF
