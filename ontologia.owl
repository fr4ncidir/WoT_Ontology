        <del_property rdf:datatype="http://wot.arces.unibo.it/ontology/web_of_things#ysap">DELETE_PROPERTY:
  sparql:
    &quot;delete where {
    ?property rdf:type wot:InteractionPattern, wot:Property;
            wot:hasName ?oldName;
            wot:hasStability ?oldStability;
