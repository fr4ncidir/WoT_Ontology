    <!-- http://wot.arces.unibo.it/ontology/web_of_things#Action -->

    <owl:Class rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#Action">
        <rdfs:subClassOf rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#InteractionPattern"/>
        <action rdf:datatype="http://wot.arces.unibo.it/ontology/web_of_things#ysap">DESCRIBE_ACTION:
  sparql:
    &quot;select * 
where {
  ?action rdf:type wot:Action;
            wot:hasName ?aName.
  optional {?action wot:hasInputDataSchema ?iDS.
            ?iDS wot:hasFieldSchema ?iFS.
            ?iFS rdf:type ?iFS_type.
            filter (?iFS_type != wot:FieldSchema)}.
  optional {?action wot:hasOutputDataSchema ?oDS.
            ?oDS wot:hasFieldSchema ?oFS.
            ?oFS rdf:type ?oFS_type.
            filter (?oFS_type != wot:FieldSchema)}
  optional {?action wot:forProperty ?property}
  values (?action) {(?action_uri)}
  }&quot;
  forcedBindings:
    action_uri:
      type: uri
      value: UNDEF</action>
        <new_empty_action rdf:datatype="http://wot.arces.unibo.it/ontology/web_of_things#ysap">ADD_EMPTY_ACTION:
  sparql:
    &quot;delete {
  ?action wot:hasName ?oldName}
insert {
  ?action rdf:type wot:InteractionPattern, wot:Action;
          wot:hasName ?newName}
where {
  optional {?action wot:hasName ?oldName}
  }&quot;
  forcedBindings:
    action:
      type: uri
      value: &quot;&quot;
    newName:
      type: literal
      value: &quot;&quot;</new_empty_action>
        <new_i_action rdf:datatype="http://wot.arces.unibo.it/ontology/web_of_things#ysap">ADD_I_ACTION:
  sparql:
    &quot;delete {
  ?action wot:hasName ?oldName;
          wot:hasDataSchema ?oldDSs;
          wot:hasInputDataSchema ?oldIDS }
insert {
  ?action rdf:type wot:InteractionPattern, wot:Action;
          wot:hasName ?newName;
          wot:hasDataSchema ?newDSs;
          wot:hasInputDataSchema ?newIDS }
where {
  optional {
    ?action wot:hasName ?oldName;
          wot:hasDataSchema ?oldDSs;
          wot:hasInputDataSchema ?oldIDS}
  }&quot;
  forcedBindings:
    action:
      type: uri
      value: &quot;&quot;
    newName:
      type: literal
      value: &quot;&quot;
    newDSs:
      type: uri
      value: &quot;&quot;
    newIDS:
      type: uri
      value: &quot;&quot;</new_i_action>
        <new_io_action rdf:datatype="http://wot.arces.unibo.it/ontology/web_of_things#ysap">ADD_IO_ACTION:
  sparql:
    &quot;delete {
  ?action wot:hasName ?oldName;
          wot:hasDataSchema ?oldDSs;
          wot:hasInputDataSchema ?oldIDS;
          wot:hasOutputDataSchema ?oldODS }
insert {
  ?action rdf:type wot:InteractionPattern, wot:Action;
          wot:hasName ?newName;
          wot:hasDataSchema ?newDSs;
          wot:hasInputDataSchema ?newIDS;
          wot:hasOutputDataSchema ?newODS }
where {
  optional {
    ?action wot:hasName ?oldName;
          wot:hasDataSchema ?oldDSs;
          wot:hasInputDataSchema ?oldIDS;
          wot:hasOutputDataSchema ?oldODS }
  }&quot;
  forcedBindings:
    action:
      type: uri
      value: &quot;&quot;
    newName:
      type: literal
      value: &quot;&quot;
    newDSs:
      type: uri
      value: &quot;&quot;
    newIDS:
      type: uri
      value: &quot;&quot;
    newODS:
      type: uri
      value: &quot;&quot;</new_io_action>
        <new_o_action rdf:datatype="http://wot.arces.unibo.it/ontology/web_of_things#ysap">ADD_O_ACTION:
  sparql:
    &quot;delete {
  ?action wot:hasName ?oldName;
          wot:hasDataSchema ?oldDSs;
          wot:hasOutputDataSchema ?oldODS }
insert {
  ?action rdf:type wot:InteractionPattern, wot:Action;
