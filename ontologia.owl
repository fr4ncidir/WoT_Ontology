      type: uri
      value: &quot;&quot;
    newName:
      type: literal
      value: &quot;&quot;
    newDSs:
      type: uri
      value: &quot;&quot;
    newODS:
      type: uri
      value: &quot;&quot;</new_o_action>
        <rdfs:comment xml:lang="en">An Action is the synchronous interaction of a Web Thing</rdfs:comment>
    </owl:Class>
    


