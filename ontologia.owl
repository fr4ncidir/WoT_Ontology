    <!-- http://wot.arces.unibo.it/ontology/web_of_things#DataSchema -->

    <owl:Class rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#DataSchema">
        <data_field_schema rdf:datatype="http://wot.arces.unibo.it/ontology/web_of_things#ysap">GET_DATASCHEMAS:
  sparql:
    &quot;select * 
where {
  ?ip wot:hasDataSchema ?ds.
  ?ds rdf:type ?ds_type;
      wot:hasFieldSchema ?fs.
  ?fs rdf:type ?fs_type.
  filter (?ds_type != wot:DataSchema)
  values (?ip ?ds ?ds_type ?fs) {(?ip_force ?ds_force ?ds_type_force ?fs_force)} 
  filter (?fs_type != wot:FieldSchema)
  values (?fs_type) {(?fs_type_force)} 
  }&quot;
  forcedBindings:
    ip_force:
      type: uri
      value: UNDEF
    ds_force:
      type: uri
      value: UNDEF
    ds_type_force:
      type: uri
      value: UNDEF
    fs_force:
      type: uri
      value: UNDEF
    fs_type_force:
      type: uri
      value: UNDEF</data_field_schema>
        <rdfs:comment xml:lang="en">Node from which you can get all the information needed to format inputs for an Action, or to interpret outputs from Actions and Events. Similarly, also the value of Properties have their DataSchema.</rdfs:comment>
    </owl:Class>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#DataSchema"/>
        <owl:annotatedProperty rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#data_field_schema"/>
        <owl:annotatedTarget rdf:datatype="http://wot.arces.unibo.it/ontology/web_of_things#ysap">GET_DATASCHEMAS:
  sparql:
    &quot;select * 
where {
  ?ip wot:hasDataSchema ?ds.
  ?ds rdf:type ?ds_type;
      wot:hasFieldSchema ?fs.
  ?fs rdf:type ?fs_type.
  filter (?ds_type != wot:DataSchema)
  values (?ip ?ds ?ds_type ?fs) {(?ip_force ?ds_force ?ds_type_force ?fs_force)} 
  filter (?fs_type != wot:FieldSchema)
  values (?fs_type) {(?fs_type_force)} 
  }&quot;
  forcedBindings:
    ip_force:
      type: uri
      value: UNDEF
    ds_force:
      type: uri
      value: UNDEF
    ds_type_force:
      type: uri
      value: UNDEF
    fs_force:
      type: uri
      value: UNDEF
    fs_type_force:
      type: uri
      value: UNDEF</owl:annotatedTarget>
        <rdfs:comment>Force bindings to query specific interaction patterns, dataschemas and so on</rdfs:comment>
    </owl:Axiom>
    


