    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#InteractionPattern"/>
        <owl:annotatedProperty rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#interaction_pattern"/>
        <owl:annotatedTarget rdf:datatype="http://wot.arces.unibo.it/ontology/web_of_things#ysap">DISCOVER_INTERACTION_PATTERNS:
  sparql:
