{
	"@context":
		{
			"wot": "http://wot.arces.unibo.it/sepa#",
			"rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
			"dul": "http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#",
			"ire": "http://w3c.github.io/wot/w3c-wot-td-ire.owl#",
			"rdfs": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
			"td": "http://wot.arces.unibo.it/ontology/web_of_things#",
			"xsd": "http://www.w3.org/2001/XMLSchema"
		},
	"@type":["td:Thing"],
	"name":"MyLamp",
	"interaction":[
		{
			"@type":["td:Property"],
			"name":"Status",
			"dataschema":"xsd:boolean",
			"writable":false,
			"stability":0,
			"start_value":true
		},
		{
			"@type":["td:Action"],
			"name":"Toggle",
			"connections":[
				{
					"@type":"td:forProperty",
					"item":"Status"
				}
			]
		},
		{
			"@type":["td:Action"],
			"name":"Blink",
			"input_dataschema":"xsd:integer"
		},
		{
			"@type":["td:Event","td:PropertyChangedEvent"],
			"name":"Ping",
			"output_dataschema":"xsd:integer",
			"connections":[
				{
					"@type":"td:forProperty",
					"item":"Status"
				}
			]
		}
	]
}
