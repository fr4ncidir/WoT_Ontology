    <!-- http://wot.arces.unibo.it/ontology/web_of_things#hasCompletionTimeStamp -->

    <owl:DatatypeProperty rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#hasCompletionTimeStamp">
        <rdfs:subPropertyOf rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#hasTimeStamp"/>
        <rdfs:domain rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#ActionInstance"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTimeStamp"/>
        <rdfs:comment xml:lang="en">Action completion time</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#hasConfirmationTimeStamp -->

    <owl:DatatypeProperty rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#hasConfirmationTimeStamp">
        <rdfs:subPropertyOf rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#hasTimeStamp"/>
        <rdfs:domain rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#ActionInstance"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTimeStamp"/>
        <rdfs:comment xml:lang="en">Action is being performed time</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#hasName -->

    <owl:DatatypeProperty rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#hasName">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#Action"/>
                    <rdf:Description rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#Event"/>
                    <rdf:Description rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#Property"/>
                    <rdf:Description rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#Thing"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Friendly name of the instance</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#hasRequestTimeStamp -->

    <owl:DatatypeProperty rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#hasRequestTimeStamp">
        <rdfs:subPropertyOf rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#hasTimeStamp"/>
        <rdfs:domain rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#ActionInstance"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTimeStamp"/>
        <rdfs:comment xml:lang="en">Action request time</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#hasStability -->

    <owl:DatatypeProperty rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#hasStability">
        <rdfs:domain rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#Property"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#long"/>
        <rdfs:comment xml:lang="it">La stabilità di una Property è l&apos;intervallo di tempo in media in cui il valore della Property rimane stabilmente fisso.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#hasTimeStamp -->

    <owl:DatatypeProperty rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#hasTimeStamp">
        <rdfs:domain rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#Instance"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTimeStamp"/>
        <rdfs:comment xml:lang="en">Instance creation time</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#isWritable -->

    <owl:DatatypeProperty rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#isWritable">
        <rdfs:domain rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#Property"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment xml:lang="it">Una Property è Writable se dall&apos;esterno della thing, tramite una qualche interazione, è possibile modificarne il valore.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#Action -->

    <owl:Class rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#Action">
        <rdfs:subClassOf rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#InteractionPattern"/>
        <append_action rdf:datatype="http://wot.arces.unibo.it/ontology/web_of_things#sparql">&quot;APPEND_ACTION&quot;:{&quot;sparql&quot;:&quot;INSERT {?tdesc wot:hasInteractionPattern ?nAction; rdf:type wot:Action, wot:InteractionPattern; wot:hasAction ?nAction. ?nAction wot:hasName ?nName} WHERE {?tdesc rdf:type wot:ThingDescription}&quot;,&quot;forcedBindings&quot;:{&quot;tdesc&quot;:{&quot;type&quot;:&quot;uri&quot;,&quot;value&quot;:&quot;&quot;},&quot;nAction&quot;:{&quot;type&quot;:&quot;uri&quot;,&quot;value&quot;:&quot;&quot;},&quot;nName&quot;:{&quot;type&quot;:&quot;literal&quot;,&quot;value&quot;:&quot;&quot;}}}</append_action>
        <get_action_description rdf:datatype="http://wot.arces.unibo.it/ontology/web_of_things#sparql">&quot;GET_ACTION_DESCRIPTION&quot;: {&quot;sparql&quot;: &quot;SELECT ?thing ?action ?aName ?inputDS ?outputDS ?target_property WHERE {?thing rdf:type wot:Thing; wot:hasThingDescription ?thing_description. ?thing_description wot:hasAction ?action. ?action rdf:type wot:Action; wot:hasName ?aName. OPTIONAL{?action wot:hasInputDataSchema ?inputDS}. OPTIONAL{?action wot:hasOutputDataSchema ?outputDS}. OPTIONAL{?action wot:forProperty ?target_property}. VALUES (?thing ?action) {(?thingURI ?actionURI)}}&quot;,&quot;forcedBindings&quot;: {&quot;thingURI&quot;: {&quot;type&quot;: &quot;uri&quot;,&quot;value&quot;: &quot;UNDEF&quot;},&quot;actionURI&quot;: {&quot;type&quot;: &quot;uri&quot;,&quot;value&quot;: &quot;UNDEF&quot;}}}</get_action_description>
        <rdfs:comment xml:lang="en">Actions are active interaction with Things. You ask, they perform.</rdfs:comment>
        <rdfs:comment xml:lang="it">Le Action sono le interazioni attive delle Thing.</rdfs:comment>
    </owl:Class>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#ActionInstance -->

    <owl:Class rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#ActionInstance">
        <rdfs:subClassOf rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#Instance"/>
        <subscribe_action_instance rdf:datatype="http://wot.arces.unibo.it/ontology/web_of_things#sparql">&quot;SUBSCRIBE_ACTION_INSTANCE&quot;: {&quot;sparql&quot;: &quot;SELECT ?aInstance ?input_data ?timestamp WHERE {?thing rdf:type wot:Thing. ?thing wot:hasThingDescription ?thing_description. ?thing_description wot:hasAction ?action. ?action wot:hasActionInstance ?aInstance. ?aInstance wot:hasRequestTimeStamp ?timestamp. OPTIONAL {?aInstance wot:hasInputData ?input. ?input rdf:type wot:InputData. ?input dul:hasDataValue ?input_data}}&quot;,&quot;forcedBindings&quot;: {&quot;thing&quot;: {&quot;type&quot;: &quot;uri&quot;,&quot;value&quot;: &quot;&quot;},&quot;thing_description&quot;: {&quot;type&quot;: &quot;uri&quot;,&quot;value&quot;: &quot;&quot;},&quot;action&quot;: {&quot;type&quot;: &quot;uri&quot;,&quot;value&quot;: &quot;&quot;}}}</subscribe_action_instance>
        <rdfs:comment xml:lang="en">Adding a new ActionInstance means requesting to perform the corresponding Action</rdfs:comment>
    </owl:Class>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#DataSchema -->

    <owl:Class rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#DataSchema">
        <append_dataschema rdf:datatype="http://wot.arces.unibo.it/ontology/web_of_things#sparql">&quot;APPEND_DATASCHEMA&quot;: {&quot;sparql&quot;: &quot;INSERT DATA {?iPattern wot:hasDataSchema ?dataschema. ?dataschema rdf:type wot:DataSchema}&quot;,&quot;forcedBindings&quot;: {&quot;iPattern&quot;: {&quot;type&quot;: &quot;uri&quot;,&quot;value&quot;: &quot;&quot;},&quot;dataschema&quot;: {&quot;type&quot;: &quot;uri&quot;,&quot;value&quot;: &quot;&quot;}}}</append_dataschema>
        <get_all_dataschemas rdf:datatype="http://wot.arces.unibo.it/ontology/web_of_things#sparql">&quot;GET_ALL_DATASCHEMAS&quot;: {&quot;sparql&quot;: &quot;SELECT ?thing ?interaction_pattern ?dataschema ?dataschema_type WHERE {?thing rdf:type wot:Thing; wot:hasThingDescription/wot:hasInteractionPattern ?interaction_pattern. ?interaction_pattern wot:hasDataSchema ?dataschema. ?dataschema rdf:type ?dataschema_type. FILTER (?dataschema_type != wot:DataSchema)}&quot;,&quot;forcedBindings&quot;: {}}

GET_ALL_DATASCHEMAS:
   sparql:</get_all_dataschemas>
        <update_dataschema>&quot;UPDATE_DATASCHEMA&quot;: {&quot;sparql&quot;: &quot;DELETE {?iPattern wot:hasDataSchema ?oldDataSchema. ?oldDataSchema rdf:type DataSchema. ?oldDataSchema wot:hasFieldSchema ?fieldSchema} INSERT {?iPattern wot:hasDataSchema ?newDataSchema. ?newDataSchema rdf:type wot:DataSchema} WHERE {?iPattern rdf:type wot:InteractionPattern. OPTIONAL {?iPattern wot:hasDataSchema ?oldDataSchema. ?oldDataSchema rdf:type DataSchema. ?oldDataSchema wot:hasFieldSchema ?fieldSchema}}&quot;,&quot;forcedBindings&quot;: {&quot;iPattern&quot;: {&quot;type&quot;: &quot;uri&quot;,&quot;value&quot;: &quot;&quot;},&quot;newDataSchema&quot;: {&quot;type&quot;: &quot;uri&quot;,&quot;value&quot;: &quot;&quot;}}}</update_dataschema>
        <rdfs:comment xml:lang="en">DataSchema is the superclass of every schema needed for Things to describe how they format data. Properties use also the DataSchema.</rdfs:comment>
    </owl:Class>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#Event -->

    <owl:Class rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#Event">
        <rdfs:subClassOf rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#InteractionPattern"/>
        <get_event_description rdf:datatype="http://wot.arces.unibo.it/ontology/web_of_things#sparql">&quot;GET_EVENT_DESCRIPTION&quot;: {&quot;sparql&quot;: &quot;SELECT ?thing ?event ?eName ?outputDS ?target_property WHERE {?thing rdf:type wot:Thing; wot:hasThingDescription ?thing_description. ?thing_description wot:hasEvent ?event. ?event rdf:type wot:Event; wot:hasName ?eName. OPTIONAL{?event wot:hasOutputDS ?outputDS}. OPTIONAL{?event wot:forProperty ?target_property}. VALUES (?thing ?event) {(?thingURI ?eventURI)}}&quot;,&quot;forcedBindings&quot;: {&quot;thingURI&quot;: {&quot;type&quot;: &quot;uri&quot;,&quot;value&quot;: &quot;UNDEF&quot;},&quot;eventURI&quot;: {&quot;type&quot;: &quot;uri&quot;,&quot;value&quot;: &quot;UNDEF&quot;}}}</get_event_description>
        <rdfs:comment xml:lang="en">Events are asynchronous interactions. You subscribe to Events to be notified in case something happens.</rdfs:comment>
        <rdfs:comment xml:lang="it">Gli Eventi sono le interazioni asincrone delle Thing.</rdfs:comment>
    </owl:Class>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#EventInstance -->

    <owl:Class rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#EventInstance">
        <rdfs:subClassOf rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#Instance"/>
        <subscribe_event_instance rdf:datatype="http://wot.arces.unibo.it/ontology/web_of_things#sparql">&quot;SUBSCRIBE_EVENT_INSTANCE&quot;: {&quot;sparql&quot;: &quot;SELECT ?eInstance ?output_data ?timestamp WHERE {?thing rdf:type wot:Thing. ?thing wot:hasThingDescription ?thing_description. ?thing_description wot:hasEvent ?event. ?event wot:hasEventInstance ?eInstance. ?eInstance wot:hasTimeStamp ?timestamp. OPTIONAL {?eInstance wot:hasOutputData ?output. ?output rdf:type wot:OutputData. ?output dul:hasDataValue ?output_data}}&quot;,&quot;forcedBindings&quot;: {&quot;thing&quot;: {&quot;type&quot;: &quot;uri&quot;,&quot;value&quot;: &quot;&quot;},&quot;thing_description&quot;: {&quot;type&quot;: &quot;uri&quot;,&quot;value&quot;: &quot;&quot;},&quot;event&quot;: {&quot;type&quot;: &quot;uri&quot;,&quot;value&quot;: &quot;&quot;}}}</subscribe_event_instance>
        <rdfs:comment xml:lang="en">A Thing adds a new EventInstance to notify that a new occurrence of the corresponding Event has happened.</rdfs:comment>
    </owl:Class>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#FieldSchema -->

    <owl:Class rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#FieldSchema">
        <get_all_fieldschemas rdf:datatype="http://wot.arces.unibo.it/ontology/web_of_things#sparql">&quot;GET_ALL_FIELDSCHEMAS&quot;: {&quot;sparql&quot;: &quot;SELECT ?field ?type WHERE {?field rdf:type wot:FieldSchema. ?field rdf:type ?type. FILTER (?type != wot:FieldSchema)}&quot;,&quot;forcedBindings&quot;: {&quot;type&quot;:{&quot;type&quot;:&quot;uri&quot;,&quot;value&quot;:&quot;&quot;}}}</get_all_fieldschemas>
        <rdfs:comment xml:lang="en">Every DataSchema can contain more than one object, each formatted in a specific way. We call them the Fields. They can be:
1) pieces of a graph, that you can query using an ontology;
2) data complex as needed, in format xsd:Literal or simpler xsd:integer;
3) web resources.
FieldSchema with its 3 subclasses allows to identify which case we are in.</rdfs:comment>
        <rdfs:comment xml:lang="it">Ogni DataSchema può contenere più oggetti, ciascuno formattato a modo suo, i cosiddetti Fields. Essi possono essere:
1) dei pezzi di grafo, interpretabili tramite l&apos;uso di un&apos;ontologia; 
2) dei dati più o meno complessi, schematizzabili tramite xsd:Literal, o altri tipi semplici; 
3) delle risorse presenti sul web. 
Il FieldSchema, con le sue sottoclassi, permette di identificare in quale dei tre casi ci troviamo.</rdfs:comment>
    </owl:Class>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#InputData -->

    <owl:Class rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#InputData">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#InformationObject"/>
        <rdfs:comment xml:lang="en">Container for all the information needed by an Action to perform its task.</rdfs:comment>
    </owl:Class>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#InputDataSchema -->

    <owl:Class rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#InputDataSchema">
        <rdfs:subClassOf rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#DataSchema"/>
        <append_dataschema rdf:datatype="http://wot.arces.unibo.it/ontology/web_of_things#sparql">&quot;APPEND_INPUT_DATASCHEMA&quot;: {&quot;sparql&quot;: &quot;INSERT DATA {?iPattern wot:hasDataSchema ?dataschema. ?dataschema rdf:type wot:DataSchema. ?dataschema rdf:type wot:InputDataSchema}&quot;,&quot;forcedBindings&quot;: {&quot;iPattern&quot;: {&quot;type&quot;: &quot;uri&quot;,&quot;value&quot;: &quot;&quot;},&quot;dataschema&quot;: {&quot;type&quot;: &quot;uri&quot;,&quot;value&quot;: &quot;&quot;}}}</append_dataschema>
        <rdfs:comment xml:lang="en">The input of an Action has to be schematized according to the schema identified by an InputDataSchema.</rdfs:comment>
    </owl:Class>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#Instance -->

    <owl:Class rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#Instance">
        <rdfs:comment xml:lang="en">Instances of Actions and Events are required to deal with dynamic evolution. A client inserts a new ActionInstance to request a Thing to perform a task.</rdfs:comment>
    </owl:Class>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#InteractionPattern -->

    <owl:Class rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#InteractionPattern">
        <discover_interaction_patterns rdf:datatype="http://wot.arces.unibo.it/ontology/web_of_things#sparql">&quot;DISCOVER_INTERACTION_PATTERNS&quot;: {&quot;sparql&quot;: &quot;SELECT ?thing ?thing_description ?interaction ?iType ?iName WHERE {?thing rdf:type wot:Thing; wot:hasThingDescription ?thing_description. ?thing_description wot:hasInteractionPattern ?interaction. ?interaction rdf:type ?iType; wot:hasName ?iName. FILTER (?iType != wot:InteractionPattern)}&quot;,&quot;forcedBindings&quot;: {}}}</discover_interaction_patterns>
        <rdfs:comment xml:lang="en">A Thing interacts with the external world with some InteractionPatterns.</rdfs:comment>
        <rdfs:comment xml:lang="it">La ThingDescription descrive l&apos;interazione della Thing tramite una collezione di InteractionPattern</rdfs:comment>
    </owl:Class>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#OntologyURI -->

    <owl:Class rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#OntologyURI">
        <rdfs:subClassOf rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#FieldSchema"/>
        <rdfs:comment xml:lang="en">An instance of OntologyURI is the actual web locationy of a specific ontology which you can use to query data necessary for an Action to run, for example.</rdfs:comment>
    </owl:Class>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#OutputData -->

    <owl:Class rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#OutputData">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#InformationObject"/>
        <rdfs:comment xml:lang="en">Container for data coming as a result of an Action, or an Event.</rdfs:comment>
    </owl:Class>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#OutputDataSchema -->

    <owl:Class rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#OutputDataSchema">
        <rdfs:subClassOf rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#DataSchema"/>
        <append_dataschema rdf:datatype="http://wot.arces.unibo.it/ontology/web_of_things#sparql">&quot;APPEND_OUTPUT_DATASCHEMA&quot;: {&quot;sparql&quot;: &quot;INSERT DATA {?iPattern wot:hasDataSchema ?dataschema. ?dataschema rdf:type wot:DataSchema. ?dataschema rdf:type wot:OutputDataSchema}&quot;,&quot;forcedBindings&quot;: {&quot;iPattern&quot;: {&quot;type&quot;: &quot;uri&quot;,&quot;value&quot;: &quot;&quot;},&quot;dataschema&quot;: {&quot;type&quot;: &quot;uri&quot;,&quot;value&quot;: &quot;&quot;}}}</append_dataschema>
        <rdfs:comment xml:lang="en">The output of Actions and Events, if present, has to be interpreted according to the format identified by the OutputDataSchema.</rdfs:comment>
    </owl:Class>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#Property -->

    <owl:Class rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#Property">
        <rdfs:subClassOf rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#InteractionPattern"/>
        <get_property_description rdf:datatype="http://wot.arces.unibo.it/ontology/web_of_things#sparql">&quot;GET_PROPERTY_DESCRIPTION&quot;: {&quot;sparql&quot;: &quot;SELECT ?thing ?property ?pName ?pStability ?pWritability ?data_schema ?property_data ?data_value WHERE {?thing rdf:type wot:Thing; wot:hasThingDescription ?thing_description. ?thing_description wot:hasProperty ?property. ?property rdf:type wot:Property; wot:hasName ?pName; wot:hasStability ?pStability; wot:isWritable ?pWritability; wot:hasDataSchema ?data_schema; wot:hasData ?property_data. ?property_data rdf:type wot:PropertyData; dul:hasDataValue ?data_value. VALUES (?thing ?property) {(?thingURI ?propertyURI)}}&quot;,&quot;forcedBindings&quot;: {&quot;thingURI&quot;: {&quot;type&quot;: &quot;uri&quot;,&quot;value&quot;: &quot;UNDEF&quot;},&quot;propertyURI&quot;: {&quot;type&quot;: &quot;uri&quot;,&quot;value&quot;: &quot;UNDEF&quot;}}}</get_property_description>
        <rdfs:comment xml:lang="en">Properties are teìhe passive interactions of Things.</rdfs:comment>
        <rdfs:comment xml:lang="it">Una Property è una caratteristica della Thing. E&apos; l&apos;oggetto di interazione passiva.</rdfs:comment>
    </owl:Class>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#PropertyData -->

    <owl:Class rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#PropertyData">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#InformationObject"/>
        <rdfs:comment xml:lang="en">Container for Property data.</rdfs:comment>
    </owl:Class>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#PropertyDataSchema -->

    <owl:Class rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#PropertyDataSchema">
        <rdfs:subClassOf rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#DataSchema"/>
    </owl:Class>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#ResourceURI -->

    <owl:Class rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#ResourceURI">
        <rdfs:subClassOf rdf:resource="http://wot.arces.unibo.it/ontology/web_of_things#FieldSchema"/>
        <rdfs:comment xml:lang="en">An instance or ResourceUri is any web resource URI. An action receiving that kind of data retrieves the resource and then performs the action.</rdfs:comment>
    </owl:Class>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#Thing -->

    <owl:Class rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#Thing">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent"/>
        <append_sub_thing rdf:datatype="http://wot.arces.unibo.it/ontology/web_of_things#sparql">&quot;APPEND_SUB_THING&quot;: {&quot;sparql&quot;: &quot;INSERT {?thing1 wot:hasSubThing ?thing2} WHERE {?thing1 rdf:type wot:Thing. ?thing2 rdf:type wot:Thing}&quot;,&quot;forcedBindings&quot;: {&quot;thing1&quot;: {&quot;type&quot;: &quot;uri&quot;,&quot;value&quot;: &quot;&quot;},&quot;thing2&quot;: {&quot;type&quot;: &quot;uri&quot;,&quot;value&quot;: &quot;&quot;}}}</append_sub_thing>
        <create_new_thing rdf:datatype="http://wot.arces.unibo.it/ontology/web_of_things#sparql">&quot;CREATE_NEW_THING&quot;: {&quot;sparql&quot;: &quot;INSERT DATA {?thing rdf:type wot:Thing, dul:Agent. ?thing wot:hasName ?name}&quot;,&quot;forcedBindings&quot;: {&quot;thing&quot;: {&quot;type&quot;: &quot;uri&quot;,&quot;value&quot;: &quot;&quot;},&quot;name&quot;:{&quot;type&quot;: &quot;literal&quot;,&quot;value&quot;: &quot;&quot;}}}</create_new_thing>
        <discover_things rdf:datatype="http://wot.arces.unibo.it/ontology/web_of_things#sparql">DISCOVER_THINGS:
   sparql: &gt;
      &quot;SELECT ?superthing ?thing ?name
       WHERE {?thing rdf:type wot:Thing; 
                           wot:hasName ?name. 
       OPTIONAL {?superthing wot:hasSubThing ?thing}
       }&quot;
   forcedBindings: 
      thingURI:
         type: &quot;uri&quot;
         value: &quot;UNDEF&quot;</discover_things>
        <rdfs:comment xml:lang="it">La classe di tutte le Web Thing. Da questo oggetto dovrebbero partire le descrizioni circostanziali legate ad altre ontologie, per identificare caratteristiche della Thing come il luogo e altre qualità.</rdfs:comment>
        <rdfs:comment xml:lang="en">This is the class of Web Things. From this object descriptions based on other ontologies should be attached, to identify location and every other circumstantial quality.</rdfs:comment>
    </owl:Class>
    


    <!-- http://wot.arces.unibo.it/ontology/web_of_things#ThingDescription -->

    <owl:Class rdf:about="http://wot.arces.unibo.it/ontology/web_of_things#ThingDescription">
        <append_thingdescription rdf:datatype="http://wot.arces.unibo.it/ontology/web_of_things#sparql">&quot;APPEND_THINGDESCRIPTION&quot;:{&quot;sparql&quot;:&quot;INSERT {?thing wot:hasThingDescription ?thingdescription. ?thingdescription rdf:type wot:ThingDescription} WHERE {?thing rdf:type wot:Thing}&quot;, &quot;forcedBindings&quot;:{&quot;thing&quot;:{&quot;type&quot;:&quot;uri&quot;,&quot;value&quot;:&quot;&quot;}, &quot;thingdescription&quot;:{&quot;type&quot;:&quot;uri&quot;,&quot;value&quot;:&quot;&quot;}}}</append_thingdescription>
        <discover_thing_descriptions rdf:datatype="http://wot.arces.unibo.it/ontology/web_of_things#sparql">&quot;DISCOVER_THING_DESCRIPTIONS&quot;: {&quot;sparql&quot;: &quot;SELECT ?thing ?thing_description WHERE {?thing rdf:type wot:Thing; wot:hasThingDescription ?thing_description. VALUES (?thing) {(?thingUri)}}&quot;, &quot;forcedBindings&quot;: {&quot;thingUri&quot;:{&quot;type&quot;:&quot;uri&quot;,&quot;value&quot;:&quot;UNDEF&quot;}}}</discover_thing_descriptions>
